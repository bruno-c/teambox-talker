<div class="sheet">
  <h2><%= @title = "Edit feed" %></h2>

  <% form_for [current_account, @feed], :html => { :class => "lw120", :autocomplete => "off" } do |f| %>
    <%= f.error_messages %>
    
    <%= render f %>
    
    <p class="indent submit">
      <%= buttons "Update", :cancel_to => account_feeds_path(current_account) %>
    </p>
  <% end %>
</div>
