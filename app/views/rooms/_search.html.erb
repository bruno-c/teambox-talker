<% form_tag account_search_room_path(current_account, @room), :method => :get, :class => "search_form", :target => "_blank" do %>
  <%= text_field_tag :q, @query, :class => "search", :type => "search", :results => 5, :placeholder => "Search logs ...", :size => 16, :accesskey => "s" %>
  <%= link_to "Browse", account_room_logs_path(current_account, @room), :title => "Browse chat logs for this room", :target => "_blank", :class => "browse" %>
<% end %>
